{
  "builds": [
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "status": 302, "headers": { "Location": "/docs" } },
    { "handle": "filesystem" },
    { "src": "/docs$", "dest": "/docs/index.html" },
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ]
}
