{
  "redirects": [
    { "source": "/", "destination": "/docs/", "permanent": false },
    { "source": "/docs", "destination": "/docs/", "permanent": true }
  ],
  "rewrites": [
    { "source": "/docs/:path*", "destination": "/docs/:path*" },
    { "source": "/openapi.json", "destination": "/openapi.json" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/:path*", "destination": "/api/:path*" }
  ]
}
